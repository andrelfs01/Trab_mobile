<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.trabalho_moveis"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.trabalho_moveis.Login_Activity"
            android:label="@string/title_activity_login_" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="br.trabalho_moveis.CriarActivity"
            android:label="@string/title_activity_criar" >
        </activity>
        <activity
            android:name="br.trabalho_moveis.Votar_Activity"
            android:label="@string/title_activity_votar_" >
        </activity>
        <activity
            android:name="br.trabalho_moveis.DailogActivity"
            android:label="@string/title_activity_dailog" >
        </activity>
        <activity
            android:name="br.trabalho_moveis.CriarItemActivity"
            android:label="@string/title_activity_criar_item" >
        </activity>
        <activity
            android:name="br.trabalho_moveis.DefinirVotoActivity"
            android:label="@string/title_activity_definir_voto" >
        </activity>
        <activity
            android:name="br.trabalho_moveis.ResultadoActivity"
            android:label="@string/title_activity_resultado" >
        </activity>
        <activity
            android:name="br.trabalho_moveis.ItemListActivity"
            android:label="@string/title_item_list" >
        </activity>
        <activity
            android:name="br.trabalho_moveis.ItemDetailActivity"
            android:label="@string/title_item_detail"
            android:parentActivityName=".ItemListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ItemListActivity" />
        </activity>
        <activity
            android:name="br.trabalho_moveis.FragmentAndroid"
            android:label="@string/title_activity_fragment_android" >
        </activity>
    </application>

</manifest>
